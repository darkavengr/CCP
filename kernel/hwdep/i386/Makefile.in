ASM=@asm@
CC=@cc@
LD=@cc@

all: @buildroot@/obj/@arch@/ex-hwdep.o @buildroot@/obj/@arch@/partition-hwdep.o @buildroot@/obj/@arch@/port-hwdep.o @buildroot@/obj/@arch@/sched-hwdep.o @buildroot@/obj/@arch@/mutex-hwdep.o @buildroot@/obj/@arch@/interrupts-hwdep.o @buildroot@/obj/@arch@/stack-hwdep.o  @buildroot@/obj/@arch@/aainit-hwdep.o @buildroot@/obj/@arch@/usermode-hwdep.o  @buildroot@/obj/@arch@/cpustate-hwdep.o @buildroot@/obj/@arch@/initializepaging-hwdep.o @buildroot@/obj/@arch@/tss-hwdep.o @buildroot@/obj/@arch@/gdt-hwdep.o @cc_opt_obj_files@
 @buildroot@/obj/@arch@/ex-hwdep.o:
		$(CC) @cc_opts@ -I@buildroot@/header -I@buildroot@/kernel/hwdep/@arch@ ex.c -o @buildroot@/obj/@arch@/ex-hwdep.o

 @buildroot@/obj/@arch@/partition-hwdep.o:
		$(CC) @cc_opts@ -I@buildroot@/header -I@buildroot@/kernel/hwdep/@arch@ partition.c -o @buildroot@/obj/@arch@/partition-hwdep.o

 @buildroot@/obj/@arch@/port-hwdep.o:
		$(CC) @cc_opts@ -I@buildroot@/header -I@buildroot@/kernel/hwdep/@arch@ port.c -o @buildroot@/obj/@arch@/port-hwdep.o

 @buildroot@/obj/@arch@/sched-hwdep.o:
		$(ASM) @asm_opts@ sched.asm -o @buildroot@/obj/@arch@/sched-hwdep.o

 @buildroot@/obj/@arch@/mutex-hwdep.o:
		$(CC) @cc_opts@ -I@buildroot@/header -I@buildroot@/kernel/hwdep/@arch@ mutex.c -o @buildroot@/obj/@arch@/mutex-hwdep.o

 @buildroot@/obj/@arch@/aainit-hwdep.o:
		$(ASM) @asm_opts@ init.asm -o @buildroot@/obj/@arch@/aainit-hwdep.o

 @buildroot@/obj/@arch@/stack-hwdep.o:
		$(ASM) @asm_opts@ stack.asm -o @buildroot@/obj/@arch@/stack-hwdep.o

 @buildroot@/obj/@arch@/tss-hwdep.o:
		$(ASM) @asm_opts@ tss.asm -o @buildroot@/obj/@arch@/tss-hwdep.o

 @buildroot@/obj/@arch@/cpustate-hwdep.o:
		$(ASM) @asm_opts@ cpustate.asm -o @buildroot@/obj/@arch@/cpustate-hwdep.o

 @buildroot@/obj/@arch@/interrupts-hwdep.o:
		$(ASM) @asm_opts@ interrupts.asm -o @buildroot@/obj/@arch@/interrupts-hwdep.o

 @buildroot@/obj/@arch@/gdt-hwdep.o:
		$(ASM) @asm_opts@ gdt.asm -o @buildroot@/obj/@arch@/gdt-hwdep.o

 @buildroot@/obj/@arch@/usermode-hwdep.o:
		$(ASM) @asm_opts@ usermode.asm -o @buildroot@/obj/@arch@/usermode-hwdep.o

 @buildroot@/obj/@arch@/initializepaging-hwdep.o:
		$(ASM) @asm_opts@ initializepaging.asm -o @buildroot@/obj/@arch@/initializepaging-hwdep.o

 @buildroot@/obj/@arch@/page-hwdep.o:
		$(CC) @cc_opts@ -I@buildroot@/header -I@buildroot@/kernel/hwdep/@arch@ page.c -o @buildroot@/obj/@arch@/page-hwdep.o

 @buildroot@/obj/@arch@/paepage-hwdep.o:
		$(CC) @cc_opts@ -I@buildroot@/header -I@buildroot@/kernel/hwdep/@arch@ paepage.c -o @buildroot@/obj/@arch@/paepage-hwdep

